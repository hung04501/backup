    .pagination {
      display: inline-block;
  }

  .pagination a {
      color: black;
      float: left;
      padding: 8px 16px;
      text-decoration: none;
  }

  .pagination a.active {
      background-color: rgb(255, 222, 0);
      color: white;
      border-radius: 50%;
  }

  .pagination a:hover:not(.active) {
      background-color: rgb(255, 222, 0);
      color: white;
      border-radius: 50%;
  }
    
    
    
    
    